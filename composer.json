{
    "name": "fiendo/payment-mollie-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 Bundle that provides access to the Mollie API. Based on JMSPaymentCoreBundle.",
    "keywords": ["payment", "mollie"],
    "homepage": "https://github.com/fiendo/PaymentMollieBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruud Kamphuis",
            "email": "ruudk@mphuis.com"
        },
        {
            "name": "Philip van Til",
            "email": "philip@fiendo.nl"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fiendo/JMSPaymentCoreBundle"
        }
    ],
    "require": {
        "php": "^5.3.2|^7.0",
        "symfony/framework-bundle": ">=2.0",
        "fiendo/payment-core-bundle": "dev-master",
        "psr/log": "~1.0",
        "omnipay/mollie": "~3.0"
    },
    "autoload": {
        "psr-4": { "Ruudk\\Payment\\MollieBundle\\": "src" }
    }
}
